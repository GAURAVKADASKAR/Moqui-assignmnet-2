        <!-- Screen for display the party data -->
        <container-dialog id="Getparty" button-text="Getparty">
        <form-single name="Getparty" transition="getpartydata">
                
            <auto-fields-entity entity-name="Party" field-type="edit" />
            
        
            <field name="submitButton">
                <default-field title="Submit"><submit/></default-field>
            </field>
        </form-single>
      
        
    </container-dialog>

        <!-- Create aparty Role -->
    <container-dialog id="PartyRolecreate" button-text="Create Party Role">
        <form-single name="Createparyrole" transition="createPartyRole">
                
